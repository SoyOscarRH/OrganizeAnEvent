{
    "compilerOptions": {
        "noEmit": false,
        "jsx": "preserve",
        "allowSyntheticDefaultImports": true,
        "alwaysStrict": true,
        "target": "esnext",
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node"
    }
}